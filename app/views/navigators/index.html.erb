<h1>Listing of Navigators</h1>

<div>
  <%= link_to 'New Navigator', new_navigator_path %>
</div>

<br>

<%= form_tag navigators_path, :method => 'get', :id => "navigators_search" do %>
  <div>
    <%= text_field_tag :search, params[:search], :placeholder => 'Search', :size => 45 %>
    <%= submit_tag "Search", :name => nil, :class => 'button big gh-black' %>
  </div>
  <div id="navigators"><%= render 'navigators' %></div>
<% end %>